{
  "name": "@tilecloud/mbgl-tilecloud-control",
  "version": "0.3.1",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "cd docs && http-server -o",
    "watch": "parcel -d ${TMPDIR} example/index.html --open",
    "build": "npm run build:lib && npm run build:site",
    "build:lib": "npm test && microbundle build src/index.js -o dist -f cjs",
    "build:site": "rm -fr docs && parcel build -d docs example/index.html && touch docs/.nojekyll",
    "test": "eslint \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tilecloud/mapbox-gl-tilecloud-control.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tilecloud/mbgl-tilecloud-control/issues"
  },
  "homepage": "https://github.com/tilecloud/mbgl-tilecloud-control#readme",
  "devDependencies": {
    "@tilecloud/mbgl-fork-me-control": "git+https://github.com/tilecloud/mbgl-fork-me-control.git",
    "eslint": "^5.4.0",
    "http-server": "^0.11.1",
    "microbundle": "^0.6.0",
    "parcel": "^1.9.7"
  }
}
